{
    "name": "kentjerone/laravel-activity-logger",
    "description": "A activity logger for models",
    "type": "library",
    "keywords": [
        "laravel",
        "logger",
        "activity logger"
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^10.0",
        "laravel/pint": "^1.16",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "KentJerone\\ActivityLogger\\": "src/"
        },
        "files": [
            "src/Helpers/activity_logger_helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "KentJerone\\ActivityLogger\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "ramosramosramos",
            "email": "ramosramosramosramos12@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
               "KentJerone\\ActivityLogger\\Providers\\ActivityLoggerProvider"
            ]
        }
    },
    "support": {
        "issues": "https://github.com/ramosramosramos/laravel-activity-logger/issues",
        "source": "https://github.com/ramosramosramos/laravel-activity-logger",
        "docs": "https://github.com/ramosramosramos/laravel-activity-logger#readme"
    },
    "minimum-stability": "stable",
    "scripts": {
        "format": [
            "vendor/bin/phpstan analyse",
            "vendor/bin/pint"
        ]
    }
}
